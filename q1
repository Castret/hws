// Online C compiler to run C program online
#include <stdio.h>
#include<math.h>
int main(){
    float x,n;
    scanf("%f%f",&x,&n);
    printf("The square root of x is : %0.f\n",sqrt(x));
    printf("x with power 2 is : %0.f\n",pow(x,2));
    printf("the upward roundoff of no n is : %0.f\n",ceil(n));
    
}
